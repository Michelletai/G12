<html>
	<head>
		<title>Verti by HTML5 UP</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="/client/assets/css/main.css" />
	</head>
	<body class="is-preload homepage">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header-wrapper">
					<header id="header" class="container">

						<!-- Logo -->
							<div id="logo">
								<h1><a href="/client/">網頁導航</a></h1>
								<span>by TKU_CSIE_GROUP-12-</span>
							</div>

						<!-- Nav -->
							<nav id="nav">
								<ul>
									<li class=""><a href="/">首頁</a></li>
									<li class="current">
										<a href="/client/weblist">網站</a>
										<ul>
											<% for(var i = 0; i < 6; i++) { 	%>
											<li><a href="<%= webSite[i].url %>"><%= webSite[i].name %></a></li>
											<% } %>
											<li><a href="/client/weblist">更多網站&nbsp;<i class="fa fa-arrow-right" aria-hidden="true"></i></a></li>
										</ul>
									</li>
									<li><a href="/admin/">家具查價網</a></li>
									<li><a href="right-sidebar.html">關於</a></li>
								</ul>
							</nav>

					</header>
				</div>

			<!-- Features -->
				<div id="features-wrapper">
					<div class="container">
						<div class="row">
							<% for(var i=0; i < webSite.length; i++) { %>
							<div class="col-4 col-12-medium">
								<!-- Box -->
									<section class="box feature">
										<a href="<%= webSite[i].url %>" class="image featured"><img class="image home" src="images/<%= webSite[i].realname %>.png" alt="" /></a>
										<div class="inner">
											<header>
												<h2><a href="<%= webSite[i].url %>"><%= webSite[i].name %></a></h2>
												<p><%= (webSite[i].desc.substring(0,64) + '...') %></p>
											</header>
										</div>
									</section>
							</div>
							<% } %>
						</div>
					</div>
				</div>

			<!-- Footer -->
				<div id="footer-wrapper-home">
					<footer id="footer" class="container">
						<div class="row">
							<div class="col-12">
								<div id="copyright">
									<ul class="menu">
										<li>&copy; TKU Group-12-. All rights reserved</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
									</ul>
								</div>
							</div>
						</div>
					</footer>
				</div>

			</div>

		<!-- Scripts -->

			<script src="/client/assets/js/jquery.min.js"></script>
			<script src="/client/assets/js/jquery.dropotron.min.js"></script>
			<script src="/client/assets/js/browser.min.js"></script>
			<script src="/client/assets/js/breakpoints.min.js"></script>
			<script src="/client/assets/js/util.js"></script>
			<script src="/client/assets/js/main.js"></script>

	</body>
</html>